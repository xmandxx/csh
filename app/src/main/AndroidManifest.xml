        <activity
            android:name=".daijia.TaximeterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".daijia.OverOrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.user.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".daijia.TempListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".daijia.EditTempActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.home.IndexActivity"
            android:label="@string/title_activity_index"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.user.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.user.ForgetPwdActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".daijia.ChooseLocationActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.business.BusinessDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.SettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.ProfileActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.personal.UserAddressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.WalletActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.RechargeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.CashActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.home.CouponsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.home.YangcheActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.home.AddYangcheActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.home.RoadescueActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.home.FuelcardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.DriverAuthActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.MyOrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personal.MyCarActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".daijia.ManagerActivity" android:screenOrientation="portrait" />
        <activity android:name=".daijia.OrderActivity" android:screenOrientation="portrait" />
        <activity android:name=".daijia.AccActivity" android:screenOrientation="portrait" />
        <activity android:name=".daijia.OrderDetailActivity" android:screenOrientation="portrait"  />
    </application>

</manifest>